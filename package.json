{
  "name": "kube-composer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://same7ammar.github.io/kube-composer/",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "deploy:aws": "npm run build && serverless deploy",
    "deploy:aws:remove": "serverless remove",
    "aws:logs": "serverless logs -f api -t"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lucide-react": "^0.263.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.1.0",
    "postcss": "^8.4.27",
    "serverless": "^3.38.0",
    "serverless-cloudfront-invalidate": "^1.12.2",
    "serverless-s3-sync": "^3.1.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}